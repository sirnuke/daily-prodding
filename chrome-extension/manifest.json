{
  "manifest_version" : 2,

  "name" : "Daily Prodding",
  "description" : "A very simple daily task tracker",
  "version" : "0.0.1",

  "permissions" :
  [
    "http://www.degrendel.com/",
    "http://www.duolingo.com/",
    "https://github.com/"
  ],
  "content_scripts" :
  [ {
    "matches" : [ "http://*/*", "https://*/*" ],
    "js" : [ "jquery-2.1.1.min.js" ]
  } ],
  "browser_action" :
  {
    "default_icon" : "icon.png",
    "default_popup" : "popup.html"
  }
}
